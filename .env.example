# Environment Configuration
NODE_ENV=development
PORT=5000

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/social_downloader

# API Keys (Add when implementing real backends)
TIKTOK_API_KEY=your_tiktok_api_key_here
INSTAGRAM_API_KEY=your_instagram_api_key_here
YOUTUBE_API_KEY=your_youtube_api_key_here
TWITTER_API_KEY=your_twitter_api_key_here

# File Storage (Optional - for cloud storage)
AWS_ACCESS_KEY_ID=your_aws_key
AWS_SECRET_ACCESS_KEY=your_aws_secret
AWS_S3_BUCKET=your_bucket_name

# Security (Optional)
JWT_SECRET=your_jwt_secret_here
RATE_LIMIT_MAX=100

# Logging
LOG_LEVEL=info